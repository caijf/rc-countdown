{
  "name": "rc-countdown",
  "version": "0.0.0",
  "description": "A simple countdown react component.",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:lib",
    "build:lib": "rm -rf lib && babel src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caijf/rc-countdown.git"
  },
  "keywords": [
    "countdown",
    "react",
    "component"
  ],
  "author": "caijf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caijf/rc-countdown/issues"
  },
  "homepage": "https://github.com/caijf/rc-countdown#readme",
  "dependencies": {
    "countdown-pro": "^1.0.0",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": [
            "> 1%",
            "last 4 versions",
            "Firefox ESR",
            "not ie < 9"
          ]
        }
      ],
      "@babel/react"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  }
}
